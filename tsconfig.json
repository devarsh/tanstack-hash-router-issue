{
  "compilerOptions": {
    "lib": ["DOM", "DOM.Iterable", "ESNext"],
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Node",
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "noUncheckedIndexedAccess": true,
    "strictNullChecks": true,
    "resolveJsonModule": true,
    "jsx": "react-jsx",
    "declaration": false,
    "noEmit": true,
    "esModuleInterop": true,
    "skipLibCheck": true
  }
}
